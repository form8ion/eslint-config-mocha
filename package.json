{
  "name": "@form8ion/eslint-config-mocha",
  "description": "shareable config for ESLint for Mocha test files",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/eslint-config-mocha",
  "bugs": "https://github.com/form8ion/eslint-config-mocha/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-mocha",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "^1.0.2",
    "@form8ion/eslint-config": "1.3.2",
    "@form8ion/remark-lint-preset": "^1.0.0",
    "ban-sensitive-files": "^1.9.2",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "remark-cli": "^6.0.1",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "eslint-plugin-mocha": "5.3.0"
  }
}
