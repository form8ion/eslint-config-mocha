{
  "name": "@form8ion/eslint-config-mocha",
  "description": "shareable config for ESLint for Mocha test files",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": "^14.17 || >=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/eslint-config-mocha",
  "bugs": "https://github.com/form8ion/eslint-config-mocha/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-mocha",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "ls-engines",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.49",
    "@form8ion/eslint-config": "5.0.29",
    "@form8ion/remark-lint-preset": "5.0.0",
    "ban-sensitive-files": "1.9.19",
    "chai": "4.3.7",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "ls-engines": "0.9.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0"
  },
  "dependencies": {
    "eslint-plugin-mocha": "10.1.0"
  }
}